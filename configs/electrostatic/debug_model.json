{
    "debug": true,
    "plot-debug-images": true,
    "cpu-device-only": false,
    "gpu-device-list": [0],
    "gpu-memory-fraction": 0.9,
    "ntasks": 2,
    "random-seed": 42,
    "input-folder": "${INVERSE_CNF_OUTPUT_DIR}/subsets/electrostatic_split_702010_batch_25",
    "output-folder": "${INVERSE_CNF_OUTPUT_DIR}/experiments/electrostatic_debug_model",
    "model-key": "cfn",
    "solver-key": "electrostatic",
    "enable-tuning": false,
    "num-epochs": 100,
    "loss-function": "smooth-l1",
    "hybrid-loss-functions": null,
    "hybrid-loss-weights": null,
    "multi-scale-kernel": null,
    "multi-scale-weights": null,
    "activation-function": "tanh",
    "projection-method": "linear",
    "projection-activation": "identity",
    "affine-log-transform": "squared_tanh",
    "affine-log-bounds": [0.0, 2.0],
    "beta-schedule-mode": "cosine",
    "beta-value-range": [0.0, 1.0],
    "beta-warmup-epochs": 5,
    "beta-schedule-epochs": 20,
    "learn-rate": 1e-4,
    "block-networks": 6,
    "hidden-layers": 3,
    "num-neurons": 128,
    "conv-kernel": 3,
    "conv-stride": 1,
    "checkpoint-frequency": 0,
    "validation-frequency": 5,
    "testing-frequency": 10,
    "testing-samples": 3,
    "generation-samples": 10,
    "generation-limit": 10,
    "generation-noise": 1.0,
    "enable-earlystop": false,
    "pruner-option": "median",
    "epoch-patience": 50,
    "trial-patience": 1,
    "threshold-upper": 0.0,
    "min-delta": 20.0
}